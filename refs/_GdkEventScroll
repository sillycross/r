<mbr r='_GdkEventScroll::x' o='192' t='gdouble'/>
<mbr r='_GdkEventScroll::y' o='256' t='gdouble'/>
<mbr r='_GdkEventScroll::state' o='320' t='guint'/>
<mbr r='_GdkEventScroll::direction' o='352' t='GdkScrollDirection'/>
<mbr r='_GdkEventScroll::device' o='384' t='GdkDevice *'/>
<mbr r='_GdkEventScroll::x_root' o='448' t='gdouble'/>
<mbr r='_GdkEventScroll::y_root' o='512' t='gdouble'/>
<mbr r='_GdkEventScroll::delta_x' o='576' t='gdouble'/>
