<mbr r='_GtkRecentChooserIface::select_uri' o='256' t='gboolean (*)(GtkRecentChooser *, const gchar *, GError **)'/>
<mbr r='_GtkRecentChooserIface::unselect_uri' o='320' t='void (*)(GtkRecentChooser *, const gchar *)'/>
<mbr r='_GtkRecentChooserIface::select_all' o='384' t='void (*)(GtkRecentChooser *)'/>
<mbr r='_GtkRecentChooserIface::unselect_all' o='448' t='void (*)(GtkRecentChooser *)'/>
<mbr r='_GtkRecentChooserIface::get_items' o='512' t='GList *(*)(GtkRecentChooser *)'/>
<mbr r='_GtkRecentChooserIface::get_recent_manager' o='576' t='GtkRecentManager *(*)(GtkRecentChooser *)'/>
