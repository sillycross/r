<mbr r='_GdkEventCrossing::time' o='256' t='guint32'/>
<mbr r='_GdkEventCrossing::x' o='320' t='gdouble'/>
<mbr r='_GdkEventCrossing::y' o='384' t='gdouble'/>
<mbr r='_GdkEventCrossing::x_root' o='448' t='gdouble'/>
<mbr r='_GdkEventCrossing::y_root' o='512' t='gdouble'/>
<mbr r='_GdkEventCrossing::mode' o='576' t='GdkCrossingMode'/>
<mbr r='_GdkEventCrossing::detail' o='608' t='GdkNotifyType'/>
<mbr r='_GdkEventCrossing::focus' o='640' t='gboolean'/>
