<mbr r='_GdkEventPadButton::send_event' o='128' t='gint8'/>
<mbr r='_GdkEventPadButton::time' o='160' t='guint32'/>
<mbr r='_GdkEventPadButton::group' o='192' t='guint'/>
<mbr r='_GdkEventPadButton::button' o='224' t='guint'/>
<mbr r='_GdkEventPadButton::mode' o='256' t='guint'/>
