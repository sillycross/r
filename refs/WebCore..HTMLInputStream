<fun r='_ZNK7WebCore15HTMLInputStream17haveSeenEndOfFileEv'/>
<fun r='_ZN7WebCore15HTMLInputStream7currentEv'/>
<fun r='_ZNK7WebCore15HTMLInputStream7currentEv'/>
<fun r='_ZN7WebCore15HTMLInputStream9splitIntoERNS_15SegmentedStringE'/>
