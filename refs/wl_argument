<doc f='include/wayland-util.h' l='672'>/**
 * Protocol message argument data types
 *
 * This union represents all of the argument types in the Wayland protocol wire
 * format. The protocol implementation uses wl_argument within its marshalling
 * machinery for dispatching messages between a client and a compositor.
 *
 * \sa wl_message
 * \sa wl_interface
 * \sa &lt;a href=&quot;https://wayland.freedesktop.org/docs/html/ch04.html#sect-Protocol-wire-Format&quot;&gt;Wire Format&lt;/a&gt;
 */</doc>
<mbr r='wl_argument::i' o='0' t='int32_t'/>
<mbr r='wl_argument::u' o='0' t='uint32_t'/>
<mbr r='wl_argument::f' o='0' t='wl_fixed_t'/>
<mbr r='wl_argument::s' o='0' t='const char *'/>
<mbr r='wl_argument::o' o='0' t='struct wl_object *'/>
<mbr r='wl_argument::n' o='0' t='uint32_t'/>
<mbr r='wl_argument::a' o='0' t='struct wl_array *'/>
<mbr r='wl_argument::h' o='0' t='int32_t'/>
