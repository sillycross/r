<use f='WebKit/Source/WebCore/platform/image-decoders/bmp/BMPImageReader.cpp' l='580' u='c' c='_ZN7WebCore14BMPImageReader14processRLEDataEv'/>
<def f='WebKit/Source/WebCore/platform/image-decoders/bmp/BMPImageReader.cpp' l='626' ll='726' type='BMPImageReader::ProcessingResult WebCore::BMPImageReader::processNonRLEData(bool inRLE, int numPixels)'/>
