<mbr r='_GtkToggleActionEntry::accelerator' o='192' t='const gchar *'/>
<mbr r='_GtkToggleActionEntry::tooltip' o='256' t='const gchar *'/>
<mbr r='_GtkToggleActionEntry::callback' o='320' t='GCallback'/>
