<mbr r='_GdkEventOwnerChange::send_event' o='128' t='gint8'/>
<mbr r='_GdkEventOwnerChange::owner' o='192' t='GdkWindow *'/>
<mbr r='_GdkEventOwnerChange::reason' o='256' t='GdkOwnerChange'/>
<mbr r='_GdkEventOwnerChange::selection' o='320' t='GdkAtom'/>
<mbr r='_GdkEventOwnerChange::time' o='384' t='guint32'/>
<mbr r='_GdkEventOwnerChange::selection_time' o='416' t='guint32'/>
