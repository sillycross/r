<mbr r='_GtkRecentFilterInfo::display_name' o='128' t='const gchar *'/>
<mbr r='_GtkRecentFilterInfo::mime_type' o='192' t='const gchar *'/>
<mbr r='_GtkRecentFilterInfo::applications' o='256' t='const gchar **'/>
<mbr r='_GtkRecentFilterInfo::groups' o='320' t='const gchar **'/>
<mbr r='_GtkRecentFilterInfo::age' o='384' t='gint'/>
