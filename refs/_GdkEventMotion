<mbr r='_GdkEventMotion::send_event' o='128' t='gint8'/>
<mbr r='_GdkEventMotion::time' o='160' t='guint32'/>
<mbr r='_GdkEventMotion::x' o='192' t='gdouble'/>
<mbr r='_GdkEventMotion::y' o='256' t='gdouble'/>
<mbr r='_GdkEventMotion::axes' o='320' t='gdouble *'/>
<mbr r='_GdkEventMotion::state' o='384' t='guint'/>
<mbr r='_GdkEventMotion::is_hint' o='416' t='gint16'/>
<mbr r='_GdkEventMotion::device' o='448' t='GdkDevice *'/>
<mbr r='_GdkEventMotion::x_root' o='512' t='gdouble'/>
<mbr r='_GdkEventMotion::y_root' o='576' t='gdouble'/>
