<mbr r='_GstVideoMeta::id' o='256' t='gint'/>
<mbr r='_GstVideoMeta::width' o='288' t='guint'/>
<mbr r='_GstVideoMeta::height' o='320' t='guint'/>
<mbr r='_GstVideoMeta::n_planes' o='352' t='guint'/>
