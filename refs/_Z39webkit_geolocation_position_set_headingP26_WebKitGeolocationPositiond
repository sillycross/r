<doc f='WebKit/Source/WebKit/UIProcess/API/glib/WebKitGeolocationManager.cpp' l='201'>/**
 * webkit_geolocation_position_set_heading:
 * @position: a #WebKitGeolocationPosition
 * @heading: heading in degrees
