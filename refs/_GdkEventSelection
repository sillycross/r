<mbr r='_GdkEventSelection::send_event' o='128' t='gint8'/>
<mbr r='_GdkEventSelection::selection' o='192' t='GdkAtom'/>
<mbr r='_GdkEventSelection::target' o='256' t='GdkAtom'/>
<mbr r='_GdkEventSelection::property' o='320' t='GdkAtom'/>
<mbr r='_GdkEventSelection::time' o='384' t='guint32'/>
<mbr r='_GdkEventSelection::requestor' o='448' t='GdkWindow *'/>
