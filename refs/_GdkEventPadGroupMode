<mbr r='_GdkEventPadGroupMode::window' o='64' t='GdkWindow *'/>
<mbr r='_GdkEventPadGroupMode::send_event' o='128' t='gint8'/>
<mbr r='_GdkEventPadGroupMode::time' o='160' t='guint32'/>
<mbr r='_GdkEventPadGroupMode::group' o='192' t='guint'/>
<mbr r='_GdkEventPadGroupMode::mode' o='224' t='guint'/>
