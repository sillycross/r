<mbr r='_PangoFontMapClass::load_fontset' o='1216' t='PangoFontset *(*)(PangoFontMap *, PangoContext *, const PangoFontDescription *, PangoLanguage *)'/>
<mbr r='_PangoFontMapClass::shape_engine_type' o='1280' t='const char *'/>
<mbr r='_PangoFontMapClass::get_serial' o='1344' t='guint (*)(PangoFontMap *)'/>
